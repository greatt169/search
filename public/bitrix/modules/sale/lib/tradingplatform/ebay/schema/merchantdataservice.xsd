							<Returned>Conditionally</Returned>
						</CallInfo>
						<CallInfo>
							<CallName>OrderReport</CallName>
							<Returned>Conditionally</Returned>
						</CallInfo>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
<!--
##### from file eBLBaseComponents/public/CancelDetailType.xsd #####
